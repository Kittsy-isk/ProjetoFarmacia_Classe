public class Clientes {
    private String nome;
    private String email;
    private double CPF;

    public Clientes(String nome, String email, double CPF) {
        this.nome = nome;
        this.email = email;
        this.CPF = CPF;
    }

    
    public void cadastrarCliente() {
        System.out.println("Cliente cadastrado: " + nome);
    }